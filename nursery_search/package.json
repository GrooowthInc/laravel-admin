{
  "name": "nursery_search",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "node node_modules/.bin/cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js --verbose",
    "watch": "node node_modules/.bin/cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot": "node node_modules/.bin/cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "node node_modules/.bin/cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "build:semanticui": "gulp --gulpfile resources/assets/semantic/gulpfile.js build",
    "deploy:staging": "cd deploy && php ../vendor/deployer/deployer/bin/dep deploy staging -vvv",
    "rollback:staging": "cd deploy && php ../vendor/deployer/deployer/bin/dep rollback staging -vvv",
    "deploy:production": "cd deploy && php ../vendor/deployer/deployer/bin/dep deploy production -vvv",
    "rollback:production": "cd deploy && php ../vendor/deployer/deployer/bin/dep rollback production -vvv",
    "deploy:clean": "rm -rfv .tmp"
  },
  "devDependencies": {
    "axios": "^0.15.2",
    "jquery": "^3.1.0",
    "laravel-mix": "^0.8.0",
    "lodash": "^4.16.2",
    "vue": "^2.0.1"
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "font-awesome": "^4.7.0",
    "iframe-resizer": "^3.5.9",
    "moment": "^2.17.1",
    "semantic-ui": "^2.2.7"
  }
}
