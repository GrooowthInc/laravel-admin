{
  "name": "nursery_search",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js --verbose",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "build:semanticui": "gulp --gulpfile resources/assets/semantic/gulpfile.js build",
    "deploy:staging": "cd deploy && php ../vendor/deployer/deployer/bin/dep deploy staging -vvv",
    "rollback:staging": "cd deploy && php ../vendor/deployer/deployer/bin/dep rollback staging -vvv",
    "deploy:production": "cd deploy && php ../vendor/deployer/deployer/bin/dep deploy production -vvv",
    "rollback:production": "cd deploy && php ../vendor/deployer/deployer/bin/dep rollback production -vvv",
    "deploy:clean": "rm -rfv .tmp"
  },
  "devDependencies": {
    "@types/jquery": "3.2.11",
    "axios": "0.16.2",
    "cross-env": "5.0.3",
    "jquery": "3.2.1",
    "laravel-mix": "1.4.2",
    "lodash": "4.17.4",
    "ts-loader": "2.3.2",
    "typescript": "2.4.2",
    "vue": "2.4.2",
    "vue-class-component": "5.0.2",
    "vue-router": "2.7.0"
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "font-awesome": "^4.7.0",
    "heightline": "^1.0.2",
    "iframe-resizer": "^3.5.9",
    "js-cookie": "^2.1.4",
    "moment": "^2.17.1",
    "semantic-ui": "^2.2.7",
    "slick-carousel": "^1.6.0"
  }
}
