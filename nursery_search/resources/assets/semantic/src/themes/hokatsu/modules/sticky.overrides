/*******************************
         Theme Overrides
*******************************/

.hkt-btn-sticky-box {
  margin: 30px 0 0;
  position: relative;
  height: 64px;
}

.hkt-btn-sticky {
  position: absolute;
  left: 0;
  bottom: 0;
}

.ui.sticky.is-fixed {
  position: fixed;
  left: 0;
  bottom: 0;
  padding: 15px 10px;
  width: 100%;
}